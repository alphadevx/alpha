{
    "name": "alphadevx/alpha",
    "type": "library",
    "description": "Alpha Framework is a full-stack MVC framework for PHP",
    "keywords": ["mvc","rest","http","framework","fullstack"],
    "homepage": "http://www.alphaframework.org/",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "John Collins",
            "email": "dev@alphaframework.org",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "dev@alphaframework.org",
        "issues": "https://github.com/alphadevx/alpha/issues",
        "wiki": "http://www.alphaframework.org/article/Documentation",
        "source": "https://github.com/alphadevx/alpha"
    },
    "require": {
        "php": ">=7.3.0",
        "ext-openssl": "*",
        "ext-mbstring": "*",
        "ext-sqlite3": "*",
        "ext-gd": "*",
        "tecnickcom/tcpdf": "6.2.26",
        "michelf/php-markdown": "1.8.0",
        "themattharris/tmhoauth": "0.8.4",
        "easybook/geshi": "v1.0.8.19",
        "luminous/luminous": "dev-psr-conversion"
    },
    "require-dev": {
        "phpunit/phpunit": "8.1.4",
        "php-coveralls/php-coveralls": "v2.1.0",
        "friendsofphp/php-cs-fixer": "v2.15.0"
    },
    "autoload": {
        "psr-0": {
            "Alpha": ".",
            "Alpha\\Test": "test"
        }
    },
    "config": {
      "secure-http": false
    }
}
