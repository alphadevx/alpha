{
    "name": "alphadevx/alpha",
    "type": "library",
    "description": "Alpha Framework is a full-stack MVC framework for PHP",
    "keywords": ["mvc","rest","http","framework","fullstack"],
    "homepage": "http://www.alphaframework.org/",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "John Collins",
            "email": "dev@alphaframework.org",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "dev@alphaframework.org",
        "issues": "https://github.com/alphadevx/alpha/issues",
        "wiki": "http://www.alphaframework.org/article/Documentation",
        "source": "https://github.com/alphadevx/alpha"
    },
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.php.net"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "ext-mcrypt": "*",
        "ext-mbstring": "*",
        "ext-sqlite3": "*",
        "ext-gd": "*",
        "tecnick.com/tcpdf": "6.2.9",
        "michelf/php-markdown": "1.4.0",
        "themattharris/tmhoauth": "0.8.4",
        "geshi/geshi": "dev-master",
        "pear-pear.php.net/pear": "*",
        "pear-pear.php.net/File_Find": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.2",
        "phing/phing": "2.8.2",
        "apigen/apigen": "2.8.1",
        "satooshi/php-coveralls": "dev-master",
        "fabpot/php-cs-fixer": "v1.11"
    },
    "autoload": {
        "psr-0": {
            "Alpha": ".",
            "Alpha\\Test": "test"
        }
    }
}
